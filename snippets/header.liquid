
<!-- BEGIN USF -->



{%- liquid
  assign admin_sp = request.design_mode
  assign des_header = settings.header_design 
  assign h_topbar = settings.h_topbar 
  assign h_transparent = settings.h_transparent
  if settings.h_banner and des_header == '7' 
    section 'header_banner' 
  endif
  assign check = true
  if h_topbar and des_header == '7' 
    section 'header_top'  
    assign check = false 
  endif
  if h_topbar and des_header != '7' and h_transparent and settings.h_tr_top == false
    section 'header_top'
    assign check = false 
  endif
  if t_name != 'index' 
    assign h_transparent = false 
  endif
  assign h__bgimg = settings.h__bgimg -%}

<style>
  .header__mid {
    max-width: 90%;
    margin: 0 auto;
  }
  	
 @media screen and (max-width:1024px){
  	.container{
     	padding:0; 
    }
 }
 </style> 

<header id="ntheader" class="ntheader header_{{des_header}} h_icon_{{settings.h_icon}}{% if h__bgimg != blank and des_header == '7' %} lazyloadt4s h__lz_img" data-bgset="{{ h__bgimg | img_url: '1x1' }}" data-ratio="{{h__bgimg.aspect_ratio}}" data-sizes="auto"{% else %}"{% endif %}>
	{%- if t_name == 'index' -%}<h1 class="h2 site-header__logo dn">{{- shop.name -}}</h1>{%- endif -%}
	<div class="ntheader_wrapper pr z_200">
      {%- liquid
        if h_topbar and check
          section 'header_top'
        endif
        case des_header 
         when '3' 
           section 'header_3'
         when '1' 
           section 'header_1'
         when '2' 
           section 'header_2'
         when '4'  
           section 'header_4'
         when '5' 
           section 'header_5'
         when '6' 
           section 'header_6'
         when '7' 
           section 'header_7'
         when '8'  
            if admin_sp 
            section 'hcat_nav' 
            endif
            section 'header_8'
         when '9' 
            section 'header_9'
         when '10' 
            section 'header_10'
        endcase -%}
	</div>
</header>
{% comment %}{%- if sck_ad contains 'setting' and des_header == '6' or des_header == '9' -%}<div class="hide">{%- section 'cat_menu' -%}</div>{%- endif -%}{% endcomment %}



{%- capture seo_title -%}{%- if template == 'search.wish' %}{{ 'general.meta.wishlist' | t }}{%- else -%}{{ page_title }}{%- endif -%}{%- endcapture -%}                                                                                    
<script>                                                                                                       
window.onload = function() {
    var searchCanvas = document.getElementById('nt_search_canvas');
    if (searchCanvas && !window._usfInstantSearchInited) {
        searchCanvas.addEventListener("mouseenter", function() {
            if (window._usfInstantSearchInited)
                return;
            var inputs = document.querySelectorAll('input.search_header__input');
            if (window.$)
                $(inputs).unbind();
            if (inputs.length) {
                if (window.usf && usf.settings.instantSearch.online){
                    inputs.forEach(e => usf.utils.installSearchInput(e));
                    console.log('%c USF - Instant Search inited', 'background: blue; color: white');
                }
                window._usfInstantSearchInited = 1;
            }
        })
    }
};
{%- if t_name == 'search' -%}
  document.title = '{{ seo_title }} - {{ shop.name}}';  
{%- endif -%}
</script>
<style>
#nt_search_canvas .search_header__prs,
#nt_search_canvas .mini_cart_content{ 
	display:none
}       
</style>
<!-- END USF -->

